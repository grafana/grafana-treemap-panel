{
  "name": "marcusolsson-treemap-panel",
  "version": "2.0.1",
  "description": "A panel plugin for Grafana to visualize treemaps",
  "keywords": [
    "grafana",
    "treemap"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\""
  },
  "homepage": "https://grafana.com/plugins/marcusolsson-treemap-panel",
  "bugs": "https://github.com/grafana/grafana-treemap-panel/issues",
  "repository": "github:grafana/grafana-treemap-panel",
  "author": {
    "name": "Grafana Labs",
    "url": "https://grafana.com"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "@grafana/data": "9.4.3",
    "@grafana/runtime": "9.4.3",
    "@grafana/ui": "9.4.3",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@grafana/toolkit": "9.4.3",    
    "@types/d3": "^5.7.2",
    "cspell": "6.13.3",
    "eslint": "<8",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "semver": "^7.3.8",    
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
